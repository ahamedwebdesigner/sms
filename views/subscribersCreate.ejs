

    <%- include('partials/header.ejs', data) %>


    <main class="container">
        <div class="bg-light p-5 rounded mt-3">
            <h1><%= title %></h1>
            <center><div id="result"></div></center>
            <form method="POST" action="/subscribers/create">
  
                <div class="form-group row">
                  <label class="col-4 col-form-label" for="name">name</label> 
                  <div class="col-8">
                    <input id="name" name="name" placeholder="Enter Name" type="text" class="form-control">
                  </div>
                </div>
                <div class="form-group row">
                  <label for="user_email" class="col-4 col-form-label">Email</label> 
                  <div class="col-8">
                    <input id="user_email" name="email" placeholder="Enter user email" type="text" class="form-control">
                  </div>
                </div>
                <div class="form-group row">
                  <label for="password" class="col-4 col-form-label">password</label> 
                  <div class="col-8">
                    <input id="password" name="password" type="password" class="form-control">
                  </div>
                </div> 
                <div class="form-group row">
                  <div class="offset-4 col-8">
                    <button id="create-User" name="submit" type="submit" class="btn btn-primary" value="submit">Submit</button>
                  </div>
                </div>
              </form>


        </div>


        <div class="subscribers-bar">
     
            <a class="btn btn-outline-primary" href="/subscribers/">List of subscribers</a>
      
      
         </div>

      </main>

   
      <%- include('partials/footer.ejs', data) %>
